{
    "source": {
        "include": [
            "morphic.js",
            "widgets.js",
            "blocks.js",
            "threads.js",
            "objects.js",
            "gui.js",
            "paint.js",
            "lists.js",
            "byob.js",
            "tables.js",
            "symbols.js",
            "xml.js",
            "store.js",
            "locale.js",
            "cloud.js"
        ]
    },
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc","closure"]
    },
    "opts": {
        "template": "node_modules/morphic-jsdoc-template",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "docdash": {
        "search": true,
        "collapse": true
    }
}

